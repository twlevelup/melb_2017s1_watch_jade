{
  "name": "proto-watch",
  "version": "1.1.0",
  "description": "Scaffoling for a proto watch app",
  "author": "LevelUp",
  "repository": {
    "type": "git",
    "url": "https://github.com/twlevelup/watch_edition"
  },
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --inline --hot",
    "build": "webpack",
    "test": "karma start",
    "test:dev": "karma start --auto-watch --no-single-run",
    "preci": "npm -s run stylecheck",
    "ci": "npm -s run test",
    "todos": "fixme -p client/src/js && fixme -p client/spec/",
    "stylecheck": "eslint client/src/js/ client/spec/ --fix",
    "heroku-postbuild": "webpack --progress"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-class-properties": "6.13.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "backbone": "^1.3.3",
    "css-loader": "^0.23.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "file-loader": "^0.8.5",
    "handlebars": "^4.0.6",
    "handlebars-template-loader": "^0.6.0",
    "html-webpack-plugin": "^2.26.0",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jquery": "^2.2.4",
    "json-loader": "^0.5.4",
    "node-sass": "^3.13.1",
    "sass-loader": "^3.2.3",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "watch_framework": "git://github.com/twlevelup/watch_framework.git#v1.0"
  },
  "engines": {
    "node": "7.4.0"
  },
  "devDependencies": {
    "fixme": "^0.4.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.0.0",
    "karma-eslint": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-threshold-reporter": "^0.1.15",
    "karma-webpack": "^2.0.3",
    "phantomjs-prebuilt": "^2.1.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.12.1"
  }
}
